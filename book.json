{
  "gitbook": "3.2.2",
  "title": "GreenPipes",
  "root": "docs",
  "plugins": [
    "edit-link",
    "prism",
    "-highlight",
    "github",
    "anchorjs",
    "expandable-chapters"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/MassTransit/GreenPipes/tree/master/docs",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/MassTransit/Greenipes/"
    },
    "theme-default": {
      "styles": {
        "website": "build/gitbook.css"
      }
    }
  }
}